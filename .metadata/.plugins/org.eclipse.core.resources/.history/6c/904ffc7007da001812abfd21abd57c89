					}
				}
			}
			year++;
			System.out.println("ƒата следующего дн€: " + day + " " + month + " " + " " + year);
		}
		
		if (year % 4 != 0 || year % 4 == 0 && year % 100 == 0 || year % 4 != 0 && year % 100 != 0 && year % 400 != 0) {
