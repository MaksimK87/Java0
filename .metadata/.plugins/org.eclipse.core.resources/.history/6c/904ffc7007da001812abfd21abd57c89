			if (month % 2 == 0 && month < 8 && month > 2) {
				for (int rate = 1; rate <= rateDay30; rate++) {
					if (rate == day && day < rateDay30) {
						day++;
						month++;
						break;
					} else if (day == rateDay30) {
						day = 1;
						month++;
					}
